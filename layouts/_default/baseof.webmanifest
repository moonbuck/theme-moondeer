{{- $data := site.Data.plugin_favicons.AndroidChrome -}}
{
    "name": "{{ $data.Name }}",
    "short_name": "{{ $data.Name }}",
    "icons": [
        {
            "src": "{{ $data.x192 }}",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "{{ $data.x512 }}",
            "sizes": "512x512",
            "type": "image/png"
        }
    ],
    "theme_color": "{{ $data.ThemeColor }}",
    "background_color": "{{ $data.BackgroundColor }}",
    "display": "standalone"
}